{
  "name": "tree-sitter-groovy",
  "version": "0.0.1",
  "description": "groovy grammar for tree-sitter",
  "main": "bindings/node",
  "tree-sitter": [
    {
      "scope": "source.jenkins",
      "file-types": [
        "pipeline",
        "jenkins",
        "Jenkinsfile"
      ],
      "scope": "source.groovy",
      "file-types": [
        "groovy"
      ]
    }
  ],
  "keywords": [
    "parsing",
    "incremental"
  ],
  "dependencies": {
    "nan": "^2.12.1"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.8"
  },
  "scripts": {
    "test": "tree-sitter test"
  }
}
